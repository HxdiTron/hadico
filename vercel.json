{
  "version": 2,
  "builds": [
    {
      "src": "api/*.php",
      "use": "vercel-php@0.6.0"
    }
  ],
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/api/?", "dest": "/api/index.php" }
  ],
  "env": {
    "PHP_INI_SCAN_DIR": "/vercel/path0/.vercel/builders/node_modules/@libphp/amazon-linux-2-v83/native/php/conf.d"
  }
}